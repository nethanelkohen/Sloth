{
  "expo": {
    "name": "Sloth",
    "slug": "Sloth-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-sloth.png",
    "splash": {
      "image": "./assets/sloth-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#af5abc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nw.sloth"
    }
  }
}
